<html>
    <head>
        <script>
            (function () {
                var matches = window.location.search.match(/[\?\&]admitad_uid\=(.+?)([\&\?]|$)/);
                if (!matches) {
                    return false;
                }

                var date = new Date();
                date.setTime(date.getTime() + 91 * 60 * 60 * 24 * 1000);

                document.cookie ='_aid=' + escape(matches[1]) + '; expires=' + date.toGMTString() + '; path=/';
            })();
        </script>
    </head>
    <body>
    </body>
</html>